{
  "name": "ctf-changelist-sync",
  "version": "0.0.0",
  "description": "Github Action to overlay a Contentful Sync with a \"Changelist\" of Entries. ",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "bundle": "npm run build && npm run package",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yan-kisen/ctf-changelist-sync.git"
  },
  "keywords": [
    "actions",
    "contentful"
  ],
  "types": "src/@types",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "contentful": "^7.14.6",
    "fs-extra": "^9.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.64",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.8.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.5.2",
    "jest-circus": "^26.5.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "semver": "^7.3.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.2"
  },
  "author": "yan-kisen <yan.kisen@coresecure.com> (https://coresecure.com)",
  "contributors": [
    "pablo-kropilnicki <pablo.kropilnicki@coresecure.com>"
  ],
  "private": true,
  "license": "MIT"
}
